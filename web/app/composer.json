{
    "require": {
        "robmorgan/phinx": "^0.16.0",
        "vlucas/phpdotenv": "^5.6",
        "doctrine/orm": "^3.0",
        "doctrine/dbal": "^4.0",
        "symfony/cache": "^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
